<body>
  <div class="row">
    <div class="col-lg-12" id="header-div">
        <p id="statistics-text">Statistical Reports</p>
        <p id="date-range">December 17, 2021 - December 31, 2021</p>
        <div id="date-picker-container">
          <div id="starting-date-container">
            <p class="date-text">Starting date</p>
            <input type="date" max="9999-12-31" id="starting-date">
          </div>
          <div id="ending-date-container">
            <p class="date-text">Ending date</p>
            <input type="date" max="9999-12-31" id="ending-date">
          </div> 
        </div>
        <button type="button" class="btn btn-primary" id="change-date-btn">Change Date</button>
    </div>
  </div> 
</body>




