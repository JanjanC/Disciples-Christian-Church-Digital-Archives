{{#each this}}
<div class='col-4' style='margin-bottom: 1em;' value="{{member_id}}, {{person_id}}, {{first_name}}, {{middle_name}}, {{last_name}},{{attendance_id}}">
    <div class='card witness member' data-member-info=>
        <div class='card-body'>
            <p class='card-text member-text'> 
                <span class='first_name'>{{first_name}}</span> 
                <span class='mid_name'>{{middle_name}}</span> 
                <span class='last_name'>{{last_name}}</span> 
                <input type='hidden' class='id_number' value="{{member_id}}">
                <input type='hidden' class='id_attendance' value="{{attendance_id}}">
                <input type='hidden' class='id_member' value="{{member_id}}"> 
            </p>
            <button type='button' class='fas fa-trash delMemberBtn'>
            </button> 
        </div>
     </div>
 </div>
 {{/each}}